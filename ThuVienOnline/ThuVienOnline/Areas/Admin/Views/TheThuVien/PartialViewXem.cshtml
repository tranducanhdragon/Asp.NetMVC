@model PagedList.IPagedList<Model.EF.Muon>
@using PagedList.Mvc

<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>Mã Sách Đang Mượn Của Thẻ @ViewBag.ID</th>
            <th>Số Lượng</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MaSach</td>
                <td>@item.SoLuongSach</td>
            </tr>
        }
    </tbody>
</table>