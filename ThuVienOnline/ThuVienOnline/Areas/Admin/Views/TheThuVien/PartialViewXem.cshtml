@model PagedList.IPagedList<Model.EF.GroupMuon>
@using PagedList.Mvc

<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>Mã Sách Đang Mượn Của Thẻ @ViewBag.IDThe</th>
            <th>Số Lượng</th>
            <th>Chức Năng</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MaSach</td>
                <td>@item.SoLuongSach</td>
                <td>
                    <a href="javascript:Tra(@item.IDThe, @item.MaSach)">Trả</a>
                </td>
            </tr>
        }
    </tbody>
</table>